upstream backend {
        server 127.0.0.1:8081;
}

proxy_cache_path /var/lib/nginx/proxy_cache levels=1:2 keys_zone=one:5m use_temp_path=off;


server {
        server_name localhost;
        listen 80;

        set $askme_root '/home/keith/PycharmProjectsPro/AskMe';

        location / {
                proxy_cache one;
                #proxy_ignore_headers X-Accel-Expires Expires Cache-Control;
                proxy_pass http://127.0.0.1:8081/;

        }

        location ^~ /uploads/ {
                alias $askme_root/uploads/;
        }

        location /static/ {
                root /home/keith/PycharmProjectsPro/AskMe/;
        }
}